<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.fit4u">

    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        tools:ignore="ManifestOrder" />

    <application
        android:allowBackup="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fit4U">
        <activity
            android:name=".client.StartTraningTimerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="package.StartTraningTimerActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".personalTrainer.MainMenuPT"
            android:exported="true">
            <intent-filter>
                <action android:name="package.MainMenuPT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".client.MainMenuUser"
            android:exported="true">
            <intent-filter>
                <action android:name="package.MainMenuUser" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".client.SeeNutritionalPlanActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="package.SeeNutritionalPlanActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".client.SeeWeekTrainingActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="package.SeeWeekTrainingActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".client.StartNewTrainingActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="package.StartNewTrainingActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.fit4u.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>